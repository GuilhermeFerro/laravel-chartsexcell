{
    "name": "gsferro/chartsexcell",
    "description": "Facade para criar facilmente charts utilizando o pacote Laravel Excell",
    "authors": [
        {
            "name": "Guilherme Ferro",
            "email": "gsferroti@gmail.com"
        }
    ],
    "require": {
        "maatwebsite/excel": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Gsferro\\ChartsExcell\\": "src/"
        },
        "files": [
            "Helpers\\chartsexcell.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gsferro\\ChartsExcell\\ChartsExcellServiceProvider"
            ],
            "aliases": {
                "ChartsExcell": "Gsferro\\ChartsExcell\\Facades\\ChartsExcell"
            }
        }
    }
}
